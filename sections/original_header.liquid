<div class="inner">
  <div class="logo">
    <h1>Test Store</h1>
  </div>
  <nav>
    <ul>
      <li>Home</li>
      <li>商品一覧</li>
      <li>会社概要</li>
      <li>お問い合わせ</li>
    </ul>
  </nav>
  <div class="icon">
    <div class="icon_serch"></div>
    <div class="icon_cart"></div>
  </div>
</div>

{%- style -%}
  header .inner {
    display: flex;
    justify-content: space-between;
  }

  header .inner ul li {
    margin-right: 10px;
  }

  header .inner .icon {
    display: flex;
    gap: 20px;
  }

  header .inner .icon .icon_serch {
    width: 40px;
    height: 20px;
    background-color: #f00;
  }

  header .inner .icon .icon_cart {
    width: 40px;
    height: 20px;
    background-color: #00f;
  }
{%- endstyle -%}

{% schema %}
  {
    "name": "o_header",
    "tag": "header"
  }
{% endschema %}